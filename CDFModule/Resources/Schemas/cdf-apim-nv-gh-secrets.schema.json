{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://cdf.epicalgroup.com/apim-nv-secrets.json",
  "title": "This schema represents the CDF APIM Named Value secrets configuration for GitHub environment.",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "kvSecretName": {
        "type": "string"
      },
      "ghSecretName": {
        "type": "string"
      }
    },
    "required": ["name", "kvSecretName", "ghSecretName"],
    "additionalProperties": false
  }
}
